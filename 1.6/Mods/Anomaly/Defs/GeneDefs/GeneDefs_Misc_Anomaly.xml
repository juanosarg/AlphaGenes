<?xml version="1.0" encoding="utf-8" ?>
<Defs>




	<GeneDef>
		<defName>AG_PseudoDeadlife</defName>
		<label>pseudo deadlife</label>
		<description>Carriers of this gene have a cloud of microscopic archites that induce a state of pseudo deadlife, granting great resilience to the person without outright killing them.</description>
		<iconPath>UI/Icons/Genes/AG_PseudoDeadlife</iconPath>
		<biostatCpx>1</biostatCpx>
		<biostatMet>-1</biostatMet>
		<displayCategory>AG_AnomalyCategory</displayCategory>
		<selectionWeight>0</selectionWeight>
		<descriptionHyperlinks>
			<HediffDef>AG_PseudoDeadlife</HediffDef>
		</descriptionHyperlinks>
		<missingGeneRomanceChanceFactor>0</missingGeneRomanceChanceFactor>
		<modExtensions>
			<li Class="VEF.Genes.GeneExtension">
				<backgroundPathEndogenes>UI/Icons/Genes/AG_Endogenes</backgroundPathEndogenes>
				<backgroundPathXenogenes>UI/Icons/Genes/AG_Xenogenes</backgroundPathXenogenes>
				<hediffToWholeBody>AG_PseudoDeadlife</hediffToWholeBody>
			</li>
		</modExtensions>
	</GeneDef>
	
	<GeneDef>
		<defName>AG_MeatBurst</defName>
		<label>meat burst</label>
		<description>Carriers will explode on death, liberating a small amount of fleshbeasts.</description>
		<iconPath>UI/Icons/Genes/AG_MeatBurst</iconPath>
		<displayCategory>AG_AnomalyCategory</displayCategory>
		
		
		<selectionWeight>0</selectionWeight>
		
		<biostatMet>-1</biostatMet>
		
		<modExtensions>
			<li Class="VEF.Genes.GeneExtension">
				<backgroundPathEndogenes>UI/Icons/Genes/AG_Endogenes</backgroundPathEndogenes>
				<backgroundPathXenogenes>UI/Icons/Genes/AG_Xenogenes</backgroundPathXenogenes>
				<hediffToWholeBody>AG_MeatBurst</hediffToWholeBody>
			</li>
		</modExtensions>
	</GeneDef>
	
	<GeneDef>
		<defName>AG_GreyPocketPlane</defName>
		<label>grey pocket plane</label>
		<description>Carriers will be able to access their own extradimensional space, having a safe haven away from the chaos of the Rim.</description>
		<iconPath>UI/Icons/Genes/AG_GreyPocketPlane</iconPath>
		<displayCategory>AG_AnomalyCategory</displayCategory>
		<exclusionTags>
			<li>AG_Portals</li>
		</exclusionTags>
		<abilities>
			<li>AG_GreyPocketPlane</li>
			<li>AG_DestroyPocketPlane</li>
			<li>AG_TeleportItemsPocketPlane</li>
		</abilities>
		<selectionWeight>0</selectionWeight>
		<descriptionHyperlinks>
			<AbilityDef>AG_GreyPocketPlane</AbilityDef>
		</descriptionHyperlinks>
		
		<biostatMet>-3</biostatMet>
		
		<modExtensions>
			<li Class="VEF.Genes.GeneExtension">
				<backgroundPathEndogenes>UI/Icons/Genes/AG_Endogenes</backgroundPathEndogenes>
				<backgroundPathXenogenes>UI/Icons/Genes/AG_Xenogenes</backgroundPathXenogenes>
				
			</li>
		</modExtensions>
	</GeneDef>
	
	


	




</Defs>