<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<HediffDef>
		<defName>AG_RoyalJellyProduction</defName>
		<label>royal jelly production</label>
		<description>The skin of this person constantly exudes and produces globs of royal jelly, which provides some extra food for the colony.</description>
		<hediffClass>HediffWithComps</hediffClass>
		<allowMothballIfLowPriorityWorldPawn>true</allowMothballIfLowPriorityWorldPawn>
		<scenarioCanAdd>false</scenarioCanAdd>
		<maxSeverity>1.0</maxSeverity>
		<isBad>false</isBad>
		<comps>
			<li Class="VEF.AnimalBehaviours.HediffCompProperties_Spawner">
				<thingToSpawn>VFEI2_RoyalInsectJelly</thingToSpawn>
				<spawnCount>5</spawnCount>
				<spawnIntervalRange>290000~310000</spawnIntervalRange>
				<spawnMaxAdjacent>30</spawnMaxAdjacent>
				<spawnForbidden>false</spawnForbidden>
				<showMessageIfOwned>false</showMessageIfOwned>
				
			</li>
		</comps>
	</HediffDef>
</Defs>