<?xml version="1.0" encoding="utf-8" ?>
<Defs>



	<FactionDef ParentName="OutlanderFactionBase">
		<defName>AG_OutlanderCivilUnion</defName>
		<label>civil xenohuman union</label>
		<classicIdeo>true</classicIdeo>
		<description>A loose union of townships established by genetically modified humans of all kinds. This group is generally friendly towards outsiders.</description>
		<factionIconPath>World/AG_OutlanderCivilUnion</factionIconPath>

		<xenotypeSet Inherit="False">
			<xenotypeChances>				
				<AG_RandomCustom>0.60</AG_RandomCustom>			
				<AG_Animusen>0.05</AG_Animusen>
				<AG_Helixien>0.05</AG_Helixien>
				<AG_Hiveling>0.05</AG_Hiveling>
				<AG_Taukai>0.05</AG_Taukai>
				<AG_MindDevourer>0.05</AG_MindDevourer>
				<AG_Wretch>0.025</AG_Wretch>
				<AG_Efreet>0.0125</AG_Efreet>
				<AG_Drakonori>0.0125</AG_Drakonori>
				<AG_Nereid>0.0125</AG_Nereid>
				<AG_Forsaken>0.01</AG_Forsaken>
				<AG_Fleetkind>0.005</AG_Fleetkind>
			</xenotypeChances>
			
		</xenotypeSet>
		<maxConfigurableAtWorldCreation>9999</maxConfigurableAtWorldCreation>
		<colorSpectrum>
      <li>(0.75, 0.20, 0.2)</li>
      <li>(0.45, 0.40, 0.90)</li>
    </colorSpectrum>
		<configurationListOrderPriority>10</configurationListOrderPriority>
	</FactionDef>

	<FactionDef ParentName="PirateBandBase">
		<defName>AG_XenohumanPirates</defName>
		<label>xenohuman pirate gang</label>
		<description>A loose confederation of pirate gangs established by genetically modified humans of all kinds.\n\nThey have little interest in building, or farming, preferring to take their sustenance from others using violence.\n\nTheir technology level depends mostly on who they've managed to steal from recently. Mostly they carry gunpowder weapons, though some prefer to stab victims at close range.</description>
		<factionIconPath>World/AG_XenohumanPirates</factionIconPath>
		<xenotypeSet Inherit="False">
			<xenotypeChances>
				<AG_RandomCustom>0.60</AG_RandomCustom>			
				<AG_Animusen>0.05</AG_Animusen>
				<AG_Helixien>0.05</AG_Helixien>
				<AG_Hiveling>0.05</AG_Hiveling>
				<AG_Taukai>0.05</AG_Taukai>
				<AG_MindDevourer>0.05</AG_MindDevourer>
				<AG_Wretch>0.025</AG_Wretch>
				<AG_Efreet>0.0125</AG_Efreet>
				<AG_Drakonori>0.0125</AG_Drakonori>
				<AG_Nereid>0.0125</AG_Nereid>
				<AG_Forsaken>0.01</AG_Forsaken>
				<AG_Fleetkind>0.005</AG_Fleetkind>
			</xenotypeChances>
			
		</xenotypeSet>
	</FactionDef>

</Defs>