<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<GeneDef>
		<defName>AG_ChemfuelBlood</defName>
		<label>chemfuel blood</label>
		<displayCategory>AG_BloodType</displayCategory>
		<description>Carriers of this gene have a chemfuel powered metabolism, and as such will bleed chemfuel instead of normal human blood. This blood is flammable.</description>
		<iconPath>UI/Icons/Genes/AG_ChemfuelBlood</iconPath>
		<displayOrderInCategory>10</displayOrderInCategory>
	
		<exclusionTags>
			<li>AG_BloodType</li>
			<li>BloodType</li>
			<li>Blood</li>
		</exclusionTags>
		<selectionWeight>0</selectionWeight>
		<biostatCpx>1</biostatCpx>
		<symbolPack>
			<wholeNameSymbols>
				<li>
					<symbol>chembleeder</symbol>
				</li>

			</wholeNameSymbols>
			<prefixSymbols>
				<li>
					<symbol>chem</symbol>
				</li>
				<li>
					<symbol>explo</symbol>
				</li>
				<li>
					<symbol>boom</symbol>
				</li>

			</prefixSymbols>

		</symbolPack>
		<modExtensions>
			<li Class="VEF.Genes.GeneExtension">
				<backgroundPathEndogenes>UI/Icons/Genes/AG_Endogenes</backgroundPathEndogenes>
				<backgroundPathXenogenes>UI/Icons/Genes/AG_Xenogenes</backgroundPathXenogenes>
				<customBloodThingDef>AG_Filth_Fuel</customBloodThingDef>
				<customBloodIcon>UI/Icons/Medical/AG_ChembloodIcon</customBloodIcon>
				<customBloodEffect>AG_Damage_HitChemfuel</customBloodEffect>
				<customWoundsFromFleshtype>AG_WoundsChemfuel</customWoundsFromFleshtype>
				<customBloodSmearThingDef>AG_Filth_BloodSmear_Chemfuel</customBloodSmearThingDef>
			</li>
		</modExtensions>
	</GeneDef>

