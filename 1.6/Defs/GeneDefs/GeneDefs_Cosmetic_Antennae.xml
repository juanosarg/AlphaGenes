<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<!-- Antennae -->
	<GeneDef Name="AG_GeneAntennaBase" Abstract="True">
		<biostatCpx>0</biostatCpx>
		<endogeneCategory>Headbone</endogeneCategory>
		<displayCategory>Cosmetic</displayCategory>
		<displayOrderInCategory>110</displayOrderInCategory>
		<exclusionTags>
			<li>Antennas</li>
			<li>Antenna</li>
			<li>AG_Antennae</li>
		</exclusionTags>
		<randomChosen>true</randomChosen>
	</GeneDef>
	<GeneDef ParentName="AG_GeneAntennaBase">
		<defName>AG_NoAntennae</defName>
		<label>no upper antennae</label>
		<description>Carriers of this gene don't have upper antennae.</description>
		<customEffectDescriptions>
			<li>With this gene you can create xenotypes in which a pawn can have upper antennae or a lack of them at random</li>
		</customEffectDescriptions>
		<iconPath>UI/Icons/Genes/AG_NoAntennae</iconPath>
		<biostatCpx>0</biostatCpx>
		<displayCategory>AG_Cosmetic_Horns</displayCategory>
		<displayOrderInCategory>-999</displayOrderInCategory>
		<selectionWeight>0</selectionWeight>
		<modExtensions>
			<li Class="VEF.Genes.GeneExtension">
				<backgroundPathEndogenes>UI/Icons/Genes/AG_Endogenes</backgroundPathEndogenes>
				<backgroundPathXenogenes>UI/Icons/Genes/AG_Xenogenes</backgroundPathXenogenes>
			</li>
		</modExtensions>
	</GeneDef>
	<GeneDef ParentName="AG_GeneAntennaBase">
		<defName>AG_MothAntennae</defName>
		<label>moth antennae</label>
		<displayCategory>AG_Cosmetic_Horns</displayCategory>
		<description>Carriers of this gene grow two large antennae protruding from the nose.</description>
		<iconPath>UI/Icons/Genes/AG_MothAntennaeIcon</iconPath>
		<selectionWeight>0</selectionWeight>
		<renderNodeProperties>
			<li>
				<nodeClass>PawnRenderNode_AttachmentHead</nodeClass>
				<workerClass>PawnRenderNodeWorker_FlipWhenCrawling</workerClass>
				<texPath>Things/Pawn/Humanlike/HeadAttachments/AG_MothAntennae/AG_MothAntennae</texPath>
				<color>(255, 255, 255)</color>
				<parentTagDef>Head</parentTagDef>
				<drawData>
					<defaultData>
						<layer>80</layer>
					</defaultData>
					<dataNorth>
						<layer>10</layer>
					</dataNorth>
				</drawData>
			</li>
		</renderNodeProperties>
		<symbolPack>
			<prefixSymbols>
				<li>
					<symbol>moth</symbol>
				</li>
				<li>
					<symbol>butterfly</symbol>
				</li>
				<li>
					<symbol>lepidopt</symbol>
				</li>
			</prefixSymbols>
		</symbolPack>
		<modExtensions>
			<li Class="VEF.Genes.GeneExtension">
				<backgroundPathEndogenes>UI/Icons/Genes/AG_Endogenes</backgroundPathEndogenes>
				<backgroundPathXenogenes>UI/Icons/Genes/AG_Xenogenes</backgroundPathXenogenes>
			</li>
		</modExtensions>
	</GeneDef>
