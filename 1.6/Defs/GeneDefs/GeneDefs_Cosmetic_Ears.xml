<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<!-- Ears -->
	<GeneDef ParentName="GeneEarsBase">
		<defName>AG_RadYakEars</defName>
		<label>radyak ears</label>
		<selectionWeight>0</selectionWeight>
		<description>Carriers of this gene will grow pointed radyak ears.</description>
		<iconPath>UI/Icons/Genes/AG_RadYakEarsIcon</iconPath>
		<renderNodeProperties>
			<li>
				<nodeClass>PawnRenderNode_AttachmentHead</nodeClass>
				<workerClass>PawnRenderNodeWorker_FlipWhenCrawling</workerClass>
				<texPath>Things/Pawn/Humanlike/HeadAttachments/AG_RadYakEars/AG_RadYakEars</texPath>
				<colorType>Skin</colorType>
				<parentTagDef>Head</parentTagDef>
				<useSkinShader>true</useSkinShader>
				<useRottenColor>true</useRottenColor>
				<rotDrawMode>Fresh, Rotting</rotDrawMode>
				<drawData>
					<defaultData>
						<layer>70</layer>
					</defaultData>
				</drawData>
			</li>
		</renderNodeProperties>
		<symbolPack>
			<prefixSymbols>
				<li>
					<symbol>radyak</symbol>
				</li>
				<li>
					<symbol>rad</symbol>
				</li>
			</prefixSymbols>
			<suffixSymbols>
				<li>
					<symbol>yak</symbol>
				</li>
				<li>
					<symbol>ear</symbol>
				</li>
			</suffixSymbols>
		</symbolPack>
		<modExtensions>
			<li Class="VEF.Genes.GeneExtension">
				<backgroundPathEndogenes>UI/Icons/Genes/AG_Endogenes</backgroundPathEndogenes>
				<backgroundPathXenogenes>UI/Icons/Genes/AG_Xenogenes</backgroundPathXenogenes>
			</li>
		</modExtensions>
	</GeneDef>
