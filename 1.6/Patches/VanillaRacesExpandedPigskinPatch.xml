<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Vanilla Races Expanded - Pigskin</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				
				<li Class="PatchOperationReplace">
					<xpath>/Defs/GeneDef[defName="VRE_PorkFlesh"]/displayCategory</xpath>
					<value>
						<displayCategory>AG_MeatType</displayCategory>
					</value>
				</li>	
				<li Class="PatchOperationReplace">
					<xpath>/Defs/GeneDef[defName="VRE_PigSkin"]/displayCategory</xpath>
					<value>
						<displayCategory>AG_LeatherType</displayCategory>
					</value>
				</li>							
			</operations>
		</match>
	</Operation>


</Patch>