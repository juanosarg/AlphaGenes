<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	
	

	<GeneDef ParentName="GeneMoodBase">
		<defName>AG_VolatileMood</defName>
		<label>volatile</label>
		<description>Carriers of this gene get random mood changes every day.</description>
		<iconPath>UI/Icons/Genes/AG_Volatile</iconPath>
		<displayOrderInCategory>1000</displayOrderInCategory>
		<customEffectDescriptions>
			<li>Mood will oscillate daily between -10 and +10.</li>			
		</customEffectDescriptions>
	
		<selectionWeight>0</selectionWeight>

		<biostatMet>1</biostatMet>
		
		<modExtensions>
			<li Class="VanillaGenesExpanded.GeneExtension">
				<backgroundPathEndogenes>UI/Icons/Genes/AG_Endogenes</backgroundPathEndogenes>
				<backgroundPathXenogenes>UI/Icons/Genes/AG_Xenogenes</backgroundPathXenogenes>
				
			</li>
		</modExtensions>
	</GeneDef>




</Defs>