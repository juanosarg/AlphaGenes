	<VREAndroids.AndroidGeneDef ParentName="AG_LEDBase">
		<defName>AG_LightLED</defName>
		<label>LED light, blue</label>
		<isCoreComponent>false</isCoreComponent>
		<description>Androids with this hardware have a blue LED in the side of their heads, betraying their synthetic nature.</description>
		<biostatCpx>0</biostatCpx>
		<selectionWeight>0</selectionWeight>
		<displayOrderInCategory>1005</displayOrderInCategory>
		<iconPath>UI/Icons/Genes/AG_BlueLEDIcon</iconPath>
		
		<renderNodeProperties>
			<li>
				<nodeClass>PawnRenderNode_AttachmentHead</nodeClass>
				<workerClass>PawnRenderNodeWorker_FlipWhenCrawling</workerClass>
				<parentTagDef>Head</parentTagDef>
				<texPath>Things/Pawn/Humanlike/HeadAttachments/AG_LightLEDs/AG_LightLED</texPath>
				<visibleFacing>
					<li>East</li>
					<li>South</li>
					<li>West</li>
				</visibleFacing>
				<narrowCrownHorizontalOffset>0.03</narrowCrownHorizontalOffset>
				<drawData>
					<defaultData>
						<layer>52</layer>
					</defaultData>
				</drawData>
			</li>
		</renderNodeProperties>
		<modExtensions Inherit="False">
			<li Class="VanillaGenesExpanded.GeneExtension">
