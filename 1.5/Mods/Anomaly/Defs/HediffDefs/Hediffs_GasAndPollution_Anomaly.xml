<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	
	
	<HediffDef>
		<defName>AG_DeadlifeProducer</defName>
		<label>deadlife dust producer</label>
		<description>This person is periodically exhale clouds of deadlife dust that will raise nearby corpses as shamblers.</description>
		<hediffClass>HediffWithComps</hediffClass>
		<scenarioCanAdd>false</scenarioCanAdd>
		<allowMothballIfLowPriorityWorldPawn>true</allowMothballIfLowPriorityWorldPawn>
		<maxSeverity>1.0</maxSeverity>
		<isBad>false</isBad>
		<comps>
			<li Class="AnimalBehaviours.HediffCompProperties_GasProducer">
				<amount>200</amount>
				<timer>60</timer>
				<gasType>DeadlifeDust</gasType>
			</li>
		</comps>
	</HediffDef>
	

	
</Defs>