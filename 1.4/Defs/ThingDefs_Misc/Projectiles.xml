<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<ThingDef ParentName="BaseBullet">
		<defName>AG_Rocks</defName>
		<label>mineral clump</label>
		<graphicData>
			<texPath>Things/Projectile/AG_Earth</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<projectile>
			<damageDef>AG_RangedBlunt</damageDef>
			<damageAmountBase>30</damageAmountBase>
			<armorPenetrationBase>1</armorPenetrationBase>
			<stoppingPower>2</stoppingPower>
			<speed>55</speed>
		</projectile>
	</ThingDef>
	
	<VFECore.ExpandableProjectileDef ParentName="BaseBullet">
		<defName>AG_Lightning</defName>
		<label>electric discharge</label>
		<thingClass>VFECore.ExpandableProjectile</thingClass>
		<graphicData>
			<texPath>Things/Projectile/Proj_Lightning</texPath>
			<texPathFadeOut>Things/Projectile/Proj_LightningFadeOut</texPathFadeOut>
			<shaderType>MoteGlow</shaderType>
		</graphicData>
		<drawOffscreen>true</drawOffscreen>
		<lifeTimeDuration>20</lifeTimeDuration>
		<widthScaleFactor>1</widthScaleFactor>
		<heightScaleFactor>1</heightScaleFactor>
		<startingPositionOffset>(0, 0, 0)</startingPositionOffset>
		<totalSizeScale>1.5</totalSizeScale>
		<tickFrameRate>4</tickFrameRate>
		<finalTickFrameRate>5</finalTickFrameRate>
		<tickDamageRate>20</tickDamageRate>
		<dealsDamageOnce>true</dealsDamageOnce>
		<minDistanceToAffect>2</minDistanceToAffect>
		<projectile>
			<damageDef>AG_Electric</damageDef>
			<flyOverhead>false</flyOverhead>
			<damageAmountBase>20</damageAmountBase>
			<stoppingPower>1.5</stoppingPower>
			<speed>100</speed>
		</projectile>
	</VFECore.ExpandableProjectileDef>
	<VFECore.ExpandableProjectileDef ParentName="BaseBullet">
		<defName>AG_GreaterFireSpew</defName>
		<label>greater fire spew</label>
		<thingClass>VFECore.FlamethrowProjectile</thingClass>
		<graphicData>
			<texPath>Things/Projectile/Proj_FireStream</texPath>
			<texPathFadeOut>Things/Projectile/Proj_FireStreamFadeOut</texPathFadeOut>
			<shaderType>MoteGlow</shaderType>
		</graphicData>
		<drawOffscreen>true</drawOffscreen>
		<lifeTimeDuration>50</lifeTimeDuration>
		<widthScaleFactor>0.65</widthScaleFactor>
		<heightScaleFactor>1</heightScaleFactor>
		<startingPositionOffset>(0,0,0)</startingPositionOffset>
		<totalSizeScale>2</totalSizeScale>
		<tickFrameRate>4</tickFrameRate>
		<finalTickFrameRate>5</finalTickFrameRate>
		<tickDamageRate>20</tickDamageRate>
		<dealsDamageOnce>true</dealsDamageOnce>
		<minDistanceToAffect>2</minDistanceToAffect>
		<projectile>
			<damageDef>Flame</damageDef>
			<flyOverhead>false</flyOverhead>
			<damageAmountBase>30</damageAmountBase>
			<stoppingPower>1.5</stoppingPower>
			<ai_IsIncendiary>true</ai_IsIncendiary>
			<speed>20</speed>
		</projectile>
	</VFECore.ExpandableProjectileDef>
</Defs>