<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<GeneDef ParentName="GenePainBase">
		<defName>AG_Painless</defName>
		<label>painless</label>
		<labelShortAdj>painless</labelShortAdj>
		<description>Carriers of this gene feel almost no pain compared to a baseliner. Reduced neuron activity in the brain's nociception centers makes pain almost unexistant. This can be advantageous sometimes, and dangerous other times.</description>
		<iconPath>UI/Icons/Genes/AG_Painless</iconPath>
		<displayOrderInCategory>40</displayOrderInCategory>
		<painFactor>0</painFactor>
		<biostatMet>-5</biostatMet>
		<symbolPack>
			<prefixSymbols>
				<li>
					<symbol>dull</symbol>
				</li>
				<li>
					<symbol>painless</symbol>
				</li>
				<li>
					<symbol>tough</symbol>
				</li>
			</prefixSymbols>
		</symbolPack>
		<selectionWeight>0</selectionWeight>
		<modExtensions>
			<li Class="VanillaGenesExpanded.GeneExtension">
				<backgroundPathEndogenes>UI/Icons/Genes/AG_Endogenes</backgroundPathEndogenes>
				<backgroundPathXenogenes>UI/Icons/Genes/AG_Xenogenes</backgroundPathXenogenes>

			</li>
		</modExtensions>
	</GeneDef>
	
	<GeneDef ParentName="GenePainBase">
		<defName>AG_BrittleBones</defName>
		<label>brittle bones</label>
		<exclusionTags>
			<li>AG_BoneType</li>
			
		</exclusionTags>
		<description>Carriers of this gene have very frail, brittle bone structure. If they are downed in any way they'll suffer cracks and fractures in their bones.</description>
		<iconPath>UI/Icons/Genes/AG_BrittleBones</iconPath>
		<displayOrderInCategory>19.9</displayOrderInCategory>
		<customEffectDescriptions>
			<li>Being downed (in any way, even medically supervised) will cause bone fractures</li>
			
		</customEffectDescriptions>
		<biostatMet>3</biostatMet>
		<symbolPack>
			<prefixSymbols>
				<li>
					<symbol>brittle</symbol>
				</li>
				<li>
					<symbol>glass</symbol>
				</li>
				<li>
					<symbol>weak</symbol>
				</li>
			</prefixSymbols>
		</symbolPack>
		<selectionWeight>0</selectionWeight>
		<modExtensions>
			<li Class="VanillaGenesExpanded.GeneExtension">
				<backgroundPathEndogenes>UI/Icons/Genes/AG_Endogenes</backgroundPathEndogenes>
				<backgroundPathXenogenes>UI/Icons/Genes/AG_Xenogenes</backgroundPathXenogenes>

			</li>
		</modExtensions>
	</GeneDef>



</Defs>