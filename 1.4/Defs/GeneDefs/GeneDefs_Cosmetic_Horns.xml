<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<!-- Horns -->
	<GeneDef ParentName="GeneHeadboneBase">
		<defName>AG_ChameleonYakHorns</defName>
		<label>chameleon yak horns</label>
		<displayCategory>AG_Cosmetic_Horns</displayCategory>
		<description>Carriers of this gene grow two curved horns protruding from the forehead.</description>
		<iconPath>UI/Icons/Genes/AG_YakHornsIcon</iconPath>
		<selectionWeight>0</selectionWeight>
		<graphicData>
			<graphicPath>Things/Pawn/Humanlike/HeadAttachments/AG_YakHorns/AG_YakHorns</graphicPath>
			<color>(255, 255, 255)</color>
			<layer>PostHeadgear</layer>
		</graphicData>
		<symbolPack>
			<prefixSymbols>
				<li>
					<symbol>chameleon yak</symbol>
				</li>
				<li>
					<symbol>yak</symbol>
				</li>
			</prefixSymbols>
			<suffixSymbols>
				<li>
					<symbol>horn</symbol>
				</li>
			</suffixSymbols>
		</symbolPack>
		<modExtensions>
			<li Class="VanillaGenesExpanded.GeneExtension">
				<backgroundPathEndogenes>UI/Icons/Genes/AG_Endogenes</backgroundPathEndogenes>
				<backgroundPathXenogenes>UI/Icons/Genes/AG_Xenogenes</backgroundPathXenogenes>
			</li>
		</modExtensions>
	</GeneDef>
