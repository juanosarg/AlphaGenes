<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<GeneDef>
		<defName>AG_FasterAging</defName>
		<label>faster aging</label>
		<description>Carriers of this gene will age at an increased rate. This applies to both children and adults. This has its advantages and disadvantages.</description>
		<iconPath>UI/Icons/Genes/AG_FasterAging</iconPath>
		<exclusionTags>
			<li>AG_Aging</li>
			<li>Aging</li>
		</exclusionTags>
		<displayCategory>Healing</displayCategory>
		<displayOrderInCategory>500</displayOrderInCategory>
		<biostatCpx>1</biostatCpx>
		<biostatMet>-1</biostatMet>
		<customEffectDescriptions>
			<li>Age will advance 50% faster.</li>
		</customEffectDescriptions>
		<biologicalAgeTickFactorFromAgeCurve>
			<points>
				<li>(13, 1.5)</li>
				<li>(18, 1.5)</li>
			</points>
		</biologicalAgeTickFactorFromAgeCurve>
		<selectionWeight>0</selectionWeight>
		<modExtensions>
			<li Class="VanillaGenesExpanded.GeneExtension">
				<backgroundPathEndogenes>UI/Icons/Genes/AG_Endogenes</backgroundPathEndogenes>
				<backgroundPathXenogenes>UI/Icons/Genes/AG_Xenogenes</backgroundPathXenogenes>
			</li>
		</modExtensions>
	</GeneDef>
	<GeneDef>
		<defName>AG_SlowerAging</defName>
		<label>slower aging</label>
		<description>Carriers of this gene will age at a decreased rate. This applies to both children and adults. This has its advantages and disadvantages.</description>
		<iconPath>UI/Icons/Genes/AG_FasterAging</iconPath>
		<exclusionTags>
			<li>AG_Aging</li>
			<li>Aging</li>
		</exclusionTags>
		<displayCategory>Healing</displayCategory>
		<displayOrderInCategory>501</displayOrderInCategory>
		<biostatCpx>1</biostatCpx>
		<biostatMet>-1</biostatMet>
		<customEffectDescriptions>
			<li>Age will advance 50% slower.</li>
		</customEffectDescriptions>
		<biologicalAgeTickFactorFromAgeCurve>
			<points>
				<li>(13, 0.5)</li>
				<li>(18, 0.5)</li>
			</points>
		</biologicalAgeTickFactorFromAgeCurve>
		<selectionWeight>0</selectionWeight>
		<modExtensions>
			<li Class="VanillaGenesExpanded.GeneExtension">
				<backgroundPathEndogenes>UI/Icons/Genes/AG_Endogenes</backgroundPathEndogenes>
				<backgroundPathXenogenes>UI/Icons/Genes/AG_Xenogenes</backgroundPathXenogenes>
			</li>
		</modExtensions>
	</GeneDef>
</Defs>